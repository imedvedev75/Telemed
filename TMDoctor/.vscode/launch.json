    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8101",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Canary",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8101/",
            "port": 9223,
            "runtimeExecutable": "C:/Users/d038471/AppData/Local/Google/Chrome SxS/Application/chrome.exe",
            "runtimeArgs": [
                "--new-window",
                "--remote-debugging-port=9223"
            ],
            "webRoot": "${workspaceRoot}"
        }
    ]
}
